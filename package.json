{
  "name": "denuel-app-rental",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "worker": "ts-node -r tsconfig-paths/register scripts/worker/worker.ts",
    "serverless:deploy": "serverless deploy",
    "serverless:package": "serverless package"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@prisma/client": "^5.0.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@vercel/blob": "^2.0.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^2.4.0",
    "cookie": "^1.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^10.12.16",
    "ioredis": "^5.2.0",
    "jsonwebtoken": "^9.0.0",
    "mapbox-gl": "^2.13.0",
    "next": "^14.0.0",
    "nodemailer": "^6.9.4",
    "rate-limiter-flexible": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.5.0",
    "sharp": "^0.32.0",
    "stripe": "^20.1.0",
    "swr": "^2.2.0",
    "ws": "^8.18.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.0.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.19.27",
    "@types/react": "^18.2.21",
    "autoprefixer": "^10.4.14",
    "aws-sdk-client-mock": "^2.0.0",
    "es-abstract": "^1.24.1",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
